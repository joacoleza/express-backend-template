{
  "name": "express-backend-template",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node build/src/server.js",
    "predev": "npm run swagger",
    "prebuild": "npm run swagger",
    "build": "tsc",
    "dev": "concurrently \"nodemon\" \"nodemon -x npm run swagger \"",
    "swagger": "tsoa routes && tsoa spec",
    "lint": "eslint . --ext .ts",
    "prettier": "prettier --config .prettierrc ./src/**/*.ts --check",
    "prettier:fix": "prettier --config .prettierrc ./src/**/*.ts --write",
    "test": "echo \"Error: no test specified\" && exit 0",
    "prepare": "husky"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.2.0",
    "inversify": "^6.0.2",
    "inversify-binding-decorators": "^4.0.0",
    "morgan": "^1.10.0",
    "swagger-ui-express": "^5.0.0",
    "tsoa": "^6.2.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.12.11",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "husky": "^9.0.11",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "ts-node src/server.ts"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}